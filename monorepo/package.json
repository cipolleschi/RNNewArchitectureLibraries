{
  "name": "mono-repo",
  "private": true,
  "workspaces": [
    "packages/*",
    "examples/*"
  ],
  "packageManager": "yarn@4.1.1",
  "engines": {
    "node": ">=18",
    "yarn": ">=4"
  },
  "scripts": {
    "lint": "eslint \"./packages/**/src/**/*.{js,jsx,ts,tsx}\"",
    "typecheck": "tsc --noEmit --composite false",
    "test": "jest",
    "clean": "lerna run clean",
    "build": "lerna run build --no-private",
    "release": "lerna publish --no-private",
    "native": "yarn workspace react-native-turbo-monorepo-example",
    "turbo-expo": "yarn workspace turbo-expo-example"
  },
  "devDependencies": {
    "@babel/core": "^7.24.0",
    "@babel/preset-env": "^7.24.0",
    "@babel/runtime": "^7.24.0",
    "@evilmartians/lefthook": "^1.6.7",
    "@react-native/babel-preset": "0.73.21",
    "@react-native/eslint-config": "^0.73.2",
    "@react-native/metro-config": "0.73.5",
    "@react-native/typescript-config": "0.74.0",
    "@types/jest": "^29.5.12",
    "@types/react": "^18.2.66",
    "@types/react-native": "^0.72.8",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "babel-plugin-module-resolver": "^5.0.0",
    "del-cli": "^5.1.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-native": "^4.1.0",
    "expo": "^50.0.13",
    "expo-modules-core": "^1.11.12",
    "jest": "^29.7.0",
    "lerna": "^8.1.2",
    "prettier": "^3.2.5",
    "react": "18.2.0",
    "react-native": "0.73.6",
    "react-native-builder-bob": "^0.23.2",
    "typescript": "5.4.2"
  },
  "resolutions": {
    "@types/react": "^18.2.6"
  }
}
